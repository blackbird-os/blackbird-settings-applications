pkgname=blackbird-settings-applications
pkgver=$(date +%Y.%m)
pkgrel=3
pkgdesc="3rd-party application settings for Blackbird OS"
arch=('any')
url='https://github.com/blackbird-os/blackbird-settings-applications'
license=('GPL')

package() {
  cd ../..
  cp -aR etc $pkgdir/
  cp -aR usr $pkgdir/
}
